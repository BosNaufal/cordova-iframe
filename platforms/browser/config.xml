<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bosnaufal.cordovaiframe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Iframe</name>
    <description>
        Cordova Focus On Iframe
    </description>
    <author email="bosnaufalemail@bosnaufal.com" href="http://github.com/BosNaufal">
        Naufal Rabbani
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <allow-navigation href="http://plansys.co/" />
    <allow-navigation href="http://plansys.co/" subdomain="true" />
</widget>
